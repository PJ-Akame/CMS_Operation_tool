# アプリケーション設定
VITE_APP_TITLE="Astro CMS & SNS自動投稿ツール"
VITE_APP_VERSION="1.0.0"
VITE_APP_ENV=development

# GitHub API設定
VITE_GITHUB_CLIENT_ID=your_github_client_id
VITE_GITHUB_CLIENT_SECRET=your_github_client_secret

# SNS API設定
VITE_TWITTER_API_KEY=your_twitter_api_key
VITE_TWITTER_API_SECRET=your_twitter_api_secret
VITE_TWITTER_BEARER_TOKEN=your_twitter_bearer_token
VITE_YOUTUBE_CLIENT_ID=your_youtube_client_id
VITE_YOUTUBE_CLIENT_SECRET=your_youtube_client_secret
VITE_DISCORD_BOT_TOKEN=your_discord_bot_token

# Claude AI設定
VITE_CLAUDE_API_KEY=your_claude_api_key

# バックエンドAPI設定（将来的に）
VITE_API_BASE_URL=http://localhost:8000/api
VITE_API_TIMEOUT=10000

# デバッグ設定
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=info